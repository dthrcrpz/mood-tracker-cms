/**
 * Avatar */
.avatar
    position: relative
    width: 75px
    height: 75px
    border-radius: 50%
    &.primary
        background-color: rgba(255, 144, 0, 0.2)
    &.secondary
        background-color: rgba(255, 68, 0, 0.2)
    &.success
        background-color: rgba(0, 160, 20, 0.2)
    &.cancel
        background-color: rgba(234, 84, 85, 0.2)
    &.warning
        background-color: rgba(255, 200, 0, 0.2)
    &.info
        background-color: rgba(67, 142, 255, 0.2)
    &.dark
        background-color: rgba(122, 122, 122, 0.2)
    .overlap
        position: absolute
        top: 50%
        left: 0
        right: 0
        text-align: center
        margin: 0 auto
        transform: translateY(-50%)

/**
 * Form */
.form
    position: relative
    &.flex
        flex: 1 0 auto
    .validate
        position: absolute
        top: calc(100% + 5px)
        background-color: $red
        color: $white
        brandon_regular(14px, false)
        z-index: 0
        padding: 5px 10px
        border-radius: 5px
        user-select: none
        right: 0
        &::before
            content: ""
            position: absolute
            bottom: calc(100% - 1px)
            width: 0
            height: 0
            border-left: 7px solid transparent
            border-right: 7px solid transparent
            border-bottom: 7px solid $red
            right: 17px
            z-index: 1
    .group_inline
        position: relative
        display: flex
        flex-flow: row wrap
        align-items: center
        justify-content: flex-start
        margin-bottom: 20px
        &.no_align
            align-items: unset
        &.between
            justify-content: space-between
        &.two
            justify-content: space-between
            .group
                flex: 0 0 49.33%
        &.three
            justify-content: space-between
            .group
                flex: 0 0 32.33%
        &.four
            justify-content: space-between
            .group
                flex: 0 0 24.33%
        .group
            flex: 0 0 auto
            margin-right: 20px
            margin-bottom: 0
    .call_to_action
        brandon_regular(14px, false)
        color: $gray
        transition: .3s ease-in-out
        &:hover
            color: $primary
            transition: .3s ease-in-out
    .disclaimer
        margin: 5px 0 0
        .violator
            brandon_light(14px, false)
            font-style: italic
            color: $black
            b
                brandon_bold(14px, false)
                color: $primary
                font-style: italic
    .preview_image
        display: flex
        flex-flow: row wrap
        align-items: flex-start
        margin: 30px 0 0
        &.three
            .preview
                flex: 0 0 calc(33.33% - 20px)
        &.four
            .preview
                flex: 0 0 calc(25% - 20px)
        .preview
            position: relative
            z-index: 0
            flex: 0 0 calc((100% / 6) - 20px)
            margin: 0 10px 20px
            animation: slide 1s
            &.big
                .item
                    padding: 40px 20px
            img
                width: 100%
                height: 200px
                object-fit: cover
            .item
                position: relative
                width: 100%
                padding: 20px
                background-image: linear-gradient(45deg, $dark_primary, $primary)
                span
                    position: absolute
                    top: 50%
                    left: 0
                    right: 0
                    padding: 0 10px
                    text-align: center
                    brandon_medium(16px, false)
                    color: $white
                    transform: translateY(-50%)
            .image_close
                position: absolute
                top: -3px
                right: -3px
                display: inline-block
                padding: 0 10px
                opacity: 1
                border-radius: 5px
                brandon_regular(20px, false)
                background-color: $red
                color: $white
                box-shadow: 0 0 10px rgba(0, 0, 0, 0.3)
                z-index: 2
                line-height: 1
                cursor: pointer
                transition: .3s ease-in-out
                &:hover
                    top: -6px
                    right: -6px
                    transition: .3s ease-in-out
    .file
        position: relative
        display: flex
        flex-flow: row wrap
        align-items: center
        label
            position: relative
            display: block
            width: 100%
            padding: 80px 0
            border: 2px dashed $primary
            border-radius: 2px
            p
                brandon_regular(18px, false)
                color: $gray
                position: absolute
                top: 50%
                left: 0
                right: 0
                text-align: center
                transform: translateY(-50%)
                b
                    position: relative
                    color: $primary
                    &::before
                        content: ''
                        position: absolute
                        bottom: 0
                        left: 0
                        width: 0
                        height: 1px
                        opacity: 0
                        background-color: $primary
                        transition: .3s ease-in-out
                    &:hover
                        &::before
                            width: 100%
                            opacity: 1
                            transition: .3s ease-in-out
    .form_image_group
        margin-bottom: 20px
        label
            user-select: none
            display: block
            brandon_medium(12px, true)
            font-weight: normal
            color: $primary
            margin-bottom: 2px
        .action_image
            position: absolute
            bottom: 10px
            opacity: 0
        .action_image_label
            position: relative
            text-transform: none
            cursor: pointer
            width: 100%
            background-color: $white
            border: 1px solid $primary
            padding: 10px 15px
            brandon_regular(14px, false)
            color: $gray
            outline: none
            transition: 0.3s ease-in-out
            &::before
                content: 'Browse'
                color: $primary
                position: absolute
                top: 50%
                right: 0
                padding: 10px 20px
                background-color: rgba(255, 144, 0, 0.2)
                border-left: 1px solid $gray_v4
                transform: translateY(-50%)
                transition: .3s ease-in-out
            &:hover,
            &:focus
                &::before
                    color: $white
                    background-color: $primary
                    border-left: 1px solid $primary
                    transition: .3s ease-in-out
                & ~ .action_image_remove
                    border-left: 1px solid $black
                    border-right: 1px solid $black
                    transition: .3s ease-in-out
            & ~ .action_image_remove
                cursor: pointer
                brandon_regular(14px, false)
                color: $white
                position: absolute
                top: 50%
                right: 0
                padding: 10px 20px
                background-color: rgba(234, 84, 85, 1)
                transform: translateY(-50%)
                border-left: 1px solid $primary
                border-right: 1px solid $primary
                border-top: none
                border-bottom: none
